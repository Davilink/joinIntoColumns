{
    "name": "join-line-into-columns",
    "displayName": "join into columns",
    "description": "Join multiple line into column.",
    "version": "0.0.1",
    "publisher": "davilink",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:joinLinesIntoCols.noTrim"
        , "onCommand:joinLinesIntoCols.withTrim"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "joinLinesIntoCols.noTrim"
            , "title": "Join lines into columns (no trim)"
        }
        , {
            "command": "joinLinesIntoCols.withTrim"
            , "title": "Join lines into columns (with trim)"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}